{
  "name": "resource-workspace-rcl",
  "version": "0.7.0",
  "main": "dist",
  "scripts": {
    "test": "jest",
    "start": "styleguidist server",
    "build": "styleguidist build",
    "prepublishOnly": "rm -rf ./dist && babel ./src --out-dir ./dist -s inline"
  },
  "repository": {
    "url": "https://github.com/unfoldingWord/resource-workspace-rcl"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react-grid-layout": "1.2.0",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@unfoldingword/eslint-config": "1.3.0",
    "babel-loader": "^8.2.2",
    "core-js": "3.7.0",
    "css-loader": "^5.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-17-updated": "^1.0.2",
    "eslint": "7.13.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-config-recommended": "4.1.0",
    "eslint-plugin-chai-friendly": "0.6.0",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gitea-react-toolkit": "^1.3.12",
    "jest": "^24.9.0",
    "markdown-translatable": "^1.2.6",
    "prettier": "^2.1.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-styleguidist": "^11.1.1",
    "scripture-resources-rcl": "^2.0.8",
    "style-loader": "^2.0.0",
    "translation-helps-rcl": "0.6.5",
    "webpack": "^4.44.2"
  },
  "browserslist": [
    ">1%",
    "last 1 version",
    "Firefox ESR",
    "not dead"
  ],
  "files": [
    "dist/*"
  ],
  "jest": {
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/src/**.{js,jsx,ts}",
      "!**/node_modules/**",
      "!**/__tests__/**",
      "!**/vendor/**"
    ],
    "testMatch": [
      "<rootDir>**/**.spec.js"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "cypress"
    ],
    "transformIgnorePatterns": [
      "mocks",
      "node_modules"
    ]
  }
}
